{
	"$schema": "https://dprint.dev/schemas/v0.json",
	"lineWidth": 80,
	"useTabs": true,
	"indentWidth": 4,
	"newLineKind": "lf",
	"plugins": [
		"https://plugins.dprint.dev/prettier-0.2.2.exe-plugin@63b06beba3acd51e6d23379c47db342a1a24f3d88f3c52a775e2db3426124582",
		"https://plugins.dprint.dev/json-0.12.0.wasm",
		"https://plugins.dprint.dev/typescript-0.46.1.wasm",
		"https://plugins.dprint.dev/markdown-0.8.0.wasm"
	],
	"json": {
		"commentLine.forceSpaceAfterSlashes": true,
		"ignoreNodeCommentText": "dprint-ignore"
	},
	"typescript": {
		"semiColons": "prefer",
		"quoteStyle": "alwaysDouble",
		"useBraces": "whenNotSingleLine"
	},
	"markdown": {},
	"includes": ["**/*.{js,cjs,mjs,ts,jsx,tsx,svelte,vue,json,md}"],
	"excludes": [
		"**/build",
		"**/dist",
		"**/lib",
		"**/node_modules",
		".husky/**/*",
		".svelte-kit/**/*",
		".vercel/**/*",
		".vercel_build_output/**/*",
		"**/*-lock.json"
	]
}
